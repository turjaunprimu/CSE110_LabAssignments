//Task_13


public class LabAssignment02
{
  public static void main(String[]args)
  {
  
  double hour=5;
  double minute=56;
  double second=23;
  double student_id=1064;

  double time=(hour+ (minute/60) + (second/3600));

  double kmph=(student_id/1000)/time;

  double mph=(student_id/1609)/time;

  System.out.println("Your velocity in kilometers per hour is " + kmph);
  System.out.println("Your velocity in miles per hour is " + mph);     
 }
    }






//Task_14


public class LabAssignment02
{
  public static void main(String[]args)
  {
  
  double a=8;
  double b=3;
  
  double c=Math.sqrt(((a/2)*(a/2)) + (b*b));

  double area=((3*Math.sqrt(3))/2)*c*c;
  double circumference=6*c;

  System.out.println("The area of the Hexagon=" +area);
  System.out.println("the circumference of the Hexagon=" +circumference);    
 }
    }
