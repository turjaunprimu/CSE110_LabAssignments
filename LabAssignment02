    System.out.println("The value of d is " +d);
  }
}






//Task_09


public class LabAssignment02
{
  public static void main(String[]args)
  {
    int n=6;

  int n1=n*1;
  int n2=n*2;
  int n3=n*3;
  int n4=n*4;
  int n5=n*5;
  int n6=n*6;
  int n7=n*7;
  int n8=n*8;
  int n9=n*9;
  int n10=n*10;

  System.out.println(6 + "x" + 1 + "=" + n1);
  System.out.println(6 + "x" + 2 + "=" + n2);
  System.out.println(6 + "x" + 3 + "=" + n3);
  System.out.println(6 + "x" + 4 + "=" + n4);
  System.out.println(6 + "x" + 5 + "=" + n5);
  System.out.println(6 + "x" + 6 + "=" + n6);
  System.out.println(6 + "x" + 7 + "=" + n7);
  System.out.println(6 + "x" + 8 + "=" + n8);
  System.out.println(6 + "x" + 9 + "=" + n9);
  System.out.println(6 + "x" + 10 + "=" + n10);
  }
}






