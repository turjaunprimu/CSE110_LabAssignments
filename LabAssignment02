public class LabAssignment02
{
  public static void main(String[]args)
  {
    int minutes=1234567;
    int hours=1234567/60;
    int days=hours/24;
    int year=days/365;
    
    
    System.out.println(minutes+ " minutes is approximately " +year+ " years and " +days+ " days.");
    
    
  }
}




