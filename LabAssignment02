//Task_11


public class LabAssignment02
{
  public static void main(String[]args)
  {
   
   double a=4.5;
   double b=9.5;
   double c=Math.sqrt((a*a)+(b*b));
   
   System.out.println("SinA=a/c=" + a/c);
   System.out.println("CosA=b/c=" + b/c);
   System.out.println("SinB=b/c=" + b/c);
   System.out.println("CosB=a/c=" + a/c);
  }
}





